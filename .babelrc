{
  "presets": ["es2015", "react", "stage-3"],
  "plugins": [
    ["syntax-dynamic-import"],
    [
      "transform-runtime",
      {
        "helpers": false,
        "polyfill": false,
        "regenerator": true,
        "moduleName": "babel-runtime"
      }
    ],
    ["import", {
      "libraryName": "antd",
      "libraryDirectory": "es",
      "style": "css" 
    }]
  ]
}